<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="s1" class="com.spring.SpringHello.Student" abstract="true">
	<property name="email" value="jigar@gmail.com"/>
</bean>
<bean id="s2" class="com.spring.SpringHello.Student" parent="s1">
	<property name="id" value="1"/>
	<property name="fname" value="Jigar"/>
	<property name="lname" value="Thakkar"/>
</bean>
</beans>
